.lg-cell {
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
}
.lg-cell__border{
  border-bottom: 2rpx solid #F5F5F5;
}
.lg-cell__title {
  width: 110rpx;
  margin-right: 60rpx;

  display: flex;
  justify-content: space-between;
  align-items: center;
  
  color: #272727;

}
.lg-cell__value {
  flex: 1;
  color: #828282;
}

.lg-cell__arrow {
  width: 12rpx;
  height: 22rpx;
  margin-left: 20rpx;
}
